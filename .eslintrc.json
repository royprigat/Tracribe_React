{
    "extends": "airbnb",
    "env": {
        "browser": true,
        "node": true,
        "jest": true,
        "es6": true
    },
    "rules": {
        "no-unused-vars": "warn",
        "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }]
    },
    "ecmaFeatures": {
        "jsx": true
    }
}